{
	"framework": "sveltekit",
	"buildCommand": "npm run build",
	"outputDirectory": ".svelte-kit/output",
	"installCommand": "npm install",
	"devCommand": "npm run dev",
	"functions": {
		"app/**": {
			"runtime": "nodejs18.x"
		}
	},
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/"
		}
	]
}
